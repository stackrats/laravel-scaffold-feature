{
    "name": "stackrats/laravel-scaffold-feature",
    "description": "Scaffold new feature directories & files from prompts",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Stackrats\\LaravelScaffoldFeature\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stackrats",
            "email": "matt@stackrats.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-data": "^4.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/pagination": "^10.0|^11.0|^12.0",
        "laravel/prompts": "^0.3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stackrats\\LaravelScaffoldFeature\\ScaffoldFeatureServiceProvider"
            ],
            "aliases": {
                "PaginationMeta": "Stackrats\\LaravelScaffoldFeature\\Facades\\PaginationMeta"
            }
        }
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "pestphp/pest": "^3.7",
        "orchestra/testbench": "^10.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Stackrats\\LaravelScaffoldFeature\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}